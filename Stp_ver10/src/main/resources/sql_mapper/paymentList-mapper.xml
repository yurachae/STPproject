<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="PaymentList">
	<select id="paymentList" parameterType="String" resultType="paymentList">
		SELECT * FROM PAY WHERE USER_EMAIL=#{login}
	</select>
	<select id="paymentManage" parameterType="String" resultType="paymentList">
		SELECT * FROM PAY 
	</select>
	
</mapper>